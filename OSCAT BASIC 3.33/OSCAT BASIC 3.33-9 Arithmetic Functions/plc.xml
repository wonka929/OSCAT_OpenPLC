      <pou name="F_LIN" pouType="function">
        <interface>
          <returnType>
            <REAL/>
          </returnType>
          <inputVars>
            <variable name="X">
              <type>
                <REAL/>
              </type>
            </variable>
            <variable name="A">
              <type>
                <REAL/>
              </type>
            </variable>
            <variable name="B">
              <type>
                <REAL/>
              </type>
            </variable>
          </inputVars>
        </interface>
        <body>
          <ST>
            <xhtml:p><![CDATA[F_lin := A * X + B;
(* from OSCAT library www.oscat.de *)]]></xhtml:p>
          </ST>
        </body>
        <documentation>
          <xhtml:p><![CDATA[calculates the linear equation f_lin = a*x + b]]></xhtml:p>
        </documentation>
      </pou>
      <pou name="F_LIN2" pouType="function">
        <interface>
          <returnType>
            <REAL/>
          </returnType>
          <inputVars>
            <variable name="X">
              <type>
                <REAL/>
              </type>
            </variable>
            <variable name="X1">
              <type>
                <REAL/>
              </type>
            </variable>
            <variable name="X2">
              <type>
                <REAL/>
              </type>
            </variable>
            <variable name="Y1">
              <type>
                <REAL/>
              </type>
            </variable>
            <variable name="Y2">
              <type>
                <REAL/>
              </type>
            </variable>
          </inputVars>
        </interface>
        <body>
          <ST>
            <xhtml:p><![CDATA[F_LIN2 := (Y2 - Y1) / (X2 - X1) * (X - X1) + Y1;
(* from OSCAT library www.oscat.de *)]]></xhtml:p>
          </ST>
        </body>
        <documentation>
          <xhtml:p><![CDATA[this function calculates the linear equation f_lin = a*x + b given by two points x1/y1 and x2/y2]]></xhtml:p>
        </documentation>
      </pou>
      <pou name="F_POLY" pouType="functionBlock">
        <interface>
